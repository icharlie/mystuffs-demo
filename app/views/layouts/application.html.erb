<%= render 'layouts/header'  %>
<body>
  <%= render 'layouts/navbar'  %>
  <div class="info">
    <% if notice %>
      <div class="alert alert-info"><%= notice %></div>
    <% end %>
    <% if alert %>
      <p class="alert"><%= alert %></p>
    <% end %>
  </div>
  <div class="container content">
    <div class="row-fluid">
      <%= yield %>
    </div>
  </div>
</div>
